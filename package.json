{
  "name": "backend-node-teste",
  "version": "1.1.0",
  "description": "NodeJS Backend API",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "npm test -- --watch -c jest-integration.config.js",
    "test:staged": "npm test --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install",
    "start": "sucrase-node src/main/server.ts"
  },
  "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joismar/backend-node-teste.git"
  },
  "author": "Joismar Braga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joismar/backend-node-teste/issues"
  },
  "homepage": "https://github.com/joismar/backend-node-teste#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.19",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "lint-staged": "^12.3.4",
    "sucrase": "^3.20.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "fast-glob": "^3.2.11",
    "firebase-admin": "^10.0.2",
    "jsonwebtoken": "^8.5.1",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  }
}
